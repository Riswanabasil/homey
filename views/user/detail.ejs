					<div itemscope itemtype="http://schema.org/Product" id="ProductSection" data-section-id="product-template" data-section-type="product-template" data-image-zoom-type="">
						<div class="wrapper">
							<div itemscope itemtype="http://schema.org/Product">
								<meta itemprop="url" content="https://digital-world-store.myshopify.com/products/example-book-3">
								<meta itemprop="image" content="//digital-world-store.myshopify.com/cdn/shop/products/Untitled-189_grande.jpg?v=1482830978">
								<div class="next-prev-products"> </div>
								<div class="grid product-single">
									<div class="grid__item large--two-fifths text-center">
										<% if (product.productImage && product.productImage.length > 0) { %>
											<div class="product-single__photos elevatezoom" id="ProductPhoto"> 
												<img src="<%= product.productImage[0] %>" alt="<%= product.productName %>" id="ProductPhotoImg" data-image-id="21477894598" data-zoom-image="<%= product.productImage[0] %>"> 
											</div>
										<% } %>
										<script>
											if($(window).innerWidth() <= 580) {
												$('#ProductPhoto img').removeAttr('id');
											}
										</script>
										<% if (product && product.productImage && product.productImage.length > 0) { %>
											<ul class="product-single__thumbnails grid-uniform" id="ProductThumbs">
												<% product.productImage.forEach((image) => { %>
													<li class="thumb__element">
														<a href="#" class="product-single__thumbnail"> 
															<img src="<%= image %>" alt="<%= product.productName %>"> 
														</a>
													</li>
												<% }) %>
											</ul>
										<% } %>
									</div>
									<script>
										$(document).ready(function(){
											$('.product-single__thumbnail').on('click', function(e) {
												e.preventDefault();
												var newImage = $(this).find('img').attr('src');
												$('#ProductPhotoImg').attr('src', newImage);
												$('#ProductPhotoImg').attr('data-zoom-image', newImage);
											});
										});
									</script>
																		
									<div class="grid__item product-single__meta--wrapper large--three-fifths">
										<div class="product-single__meta grid__item five-eighths">
											<div itemprop="offers" itemscope itemtype="http://schema.org/Offer">
												<div class="price_wrapper"> <span id="PriceA11y" class="visually-hidden">Regular price</span> <span id="ProductPrice" class="product-single__price" itemprop="price" content="349.99"><span class=money>â‚¹<%= product.salePrice %></span> </span>
													<% if (product.productOffer > 0) { %>
														<p>
															<%= product.productOffer %>% off</p>
														<% } %>
															<div class="rating"> <span class="fa fa-star checked"></span> <span class="fa fa-star checked"></span> <span class="fa fa-star checked"></span> <span class="fa fa-star"></span> <span class="fa fa-star"></span> </div>
												</div> <span class="rating"><span class="shopify-product-reviews-badge" data-id="9059380358"></span></span>
												<meta itemprop="priceCurrency" content="USD">
												<link itemprop="availability" href="http://schema.org/InStock">
												<!-- <form action="https://digital-world-store.myshopify.com/cart/add" method="post" enctype="multipart/form-data" class="product-single__form AddToCartForm" id=""> -->
													<div class="product-single__description rte" itemprop="description">
														<ul class="spec">
															<h4>Description</h4>
															<li>
																<%= product.description %>
															</li>
															<h4>Specifications</h4>
															<li>Category:
																<%= product.category.name %>
															</li>
															<li>Stock:
