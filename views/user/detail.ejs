		<script>
			function addToCart(productId) {
				fetch(`/add-to-cart/${productId}`)
					.then(response => response.json())
					.then(data => {
						if (data.success) {
							Swal.fire({
								icon: 'success',
								title: 'Success',
								text: data.message,
							}).then(() => {
								window.location.reload();
							});
						} else {
							Swal.fire({
								icon: 'error',
								title: 'Error',
								text: data.message,
							});
						}
					})
					.catch(error => {
						console.error('Error:', error);
						Swal.fire({
							icon: 'error',
							title: 'Error',
							text: 'An error occurred while adding the product to the cart',
						});
					});
			}
		</script>
